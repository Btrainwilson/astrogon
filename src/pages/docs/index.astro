---
import DocGroup from "@layouts/DocGroup.astro";
import { getEntry } from "astro:content";
import { getGroups } from "@/lib/contentParser";
import type { DocsEntry } from "@/types";

const groupIndices = await getGroups("docs") as DocsEntry[];
const collectionIndex = await getEntry("docs", "index");
---

<DocGroup entries={groupIndices} collectionIndex={collectionIndex} />
