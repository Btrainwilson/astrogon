---
import TaxonGroup from "@layouts/TaxonGroup.astro";
import { getTaxa, getTaxaMultiset } from "@/lib/taxonomyParser.astro";

const collection = "blog";
const taxonomy = "categories";
const taxa = await getTaxa(collection, taxonomy);
const allTaxa = await getTaxaMultiset(collection, taxonomy);
---

<TaxonGroup collection={collection} taxonomy={taxonomy} taxa={taxa} allTaxa={allTaxa} />
